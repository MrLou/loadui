<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:osgi="http://www.springframework.org/schema/osgi"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
                      http://www.springframework.org/schema/osgi http://www.springframework.org/schema/osgi/spring-osgi.xsd">

	<osgi:service interface="com.eviware.loadui.api.statistics.StatisticsAggregator" ref="statisticsAggregator" />
	
	<osgi:service interface="com.eviware.loadui.api.execution.TestRunner" ref="testRunner" />
	
	<osgi:service interface="com.eviware.loadui.api.testevents.TestEventManager" ref="testEventManager" />
	
	<osgi:reference interface="com.eviware.loadui.api.messaging.BroadcastMessageEndpoint" id="broadcastMessageEndpoint" />

	<osgi:reference interface="com.eviware.loadui.api.dispatch.ExecutorManager"
		id="executorManager" />

	<osgi:reference interface="java.util.concurrent.ScheduledExecutorService"
		id="scheduledExecutorService" />
		
	<osgi:reference interface="java.util.concurrent.ExecutorService"
		id="executorService" />

	<osgi:reference interface="org.springframework.core.convert.ConversionService"
		id="conversionService" />

	<osgi:reference interface="com.eviware.loadui.api.messaging.ServerEndpoint"
		id="serverEndpoint" />

	<osgi:reference interface="com.eviware.loadui.api.property.PropertySynchronizer"
		id="propertySynchronizer" />

	<osgi:reference
		interface="com.eviware.loadui.api.addressable.AddressableRegistry" id="addressableRegistry" />

	<osgi:reference interface="com.eviware.loadui.api.terminal.TerminalProxy"
		id="terminalProxy" />
		
	
	<osgi:reference
		interface="com.eviware.loadui.api.testevents.TestEventRegistry" id="testEventRegistry" />
		
	<!--<osgi:reference
		interface="com.eviware.loadui.api.statistics.store.ExecutionManager"
		id="executionManager" />-->
</beans>