<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import java.net.URL?>

<fx:root type="javafx.scene.layout.Pane" xmlns:fx="http://javafx.com/fxml">
	<stylesheets>
		<URL value="@ExecutionView.css" />
	</stylesheets>
	<VBox styleClass="execution-view" maxWidth="135">
		<HBox styleClass="topExecMenu">
			<MenuButton fx:id="menuButton" />
		</HBox>
		<Region onMouseClicked="#openExecution" minWidth="135"
			minHeight="80" styleClass="execution-background" />
	</VBox>

</fx:root>