<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.geometry.Insets?>
<?import java.net.URL?>

<VBox xmlns:fx="http://javafx.com/fxml" styleClass="analysis-view"
	fx:controller="com.eviware.loadui.ui.fx.views.analysis.AnalysisView$Controller">
	<stylesheets>
		<URL value="@AnalysisView.css" />
	</stylesheets>
	<HBox spacing="10" alignment="baseline_left">
	<Label fx:id="executionLabel" styleClass="title" text="No Execution" />
	<Button text="close" onAction="#close" />
	</HBox>
	<StackPane fx:id="chartContainer" VBox.vgrow="always" />
</VBox>