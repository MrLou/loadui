<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:osgi="http://www.springframework.org/schema/osgi"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
                      http://www.springframework.org/schema/osgi http://www.springframework.org/schema/osgi/spring-osgi-1.0.xsd">
	
	<osgi:service interface="com.eviware.loadui.api.statistics.store.ExecutionManager" ref="executionManager" />
	<osgi:service interface="com.eviware.loadui.api.TestEventRegistry" ref="testEventRegistry" />
	<osgi:service interface="com.eviware.loadui.api.testevents.TestEventManager" ref="testEventManager" />
	
	<osgi:reference interface="com.eviware.loadui.api.model.WorkspaceProvider" id="workspaceProvider" />
	
	<osgi:list id="testEventFactories" interface="com.eviware.loadui.api.testevents.TestEvent$Factory" cardinality="0..N">
		<osgi:listener ref="testEventRegistry" bind-method="factoryAdded" unbind-method="factoryRemoved" />
	</osgi:list>
</beans>