<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	
	<bean id="counterSynchronizer" class="com.eviware.loadui.impl.agent.AgentCounterSynchronizer" />
	
	<bean id="controller" class="com.eviware.loadui.impl.agent.ControllerImpl">
		<constructor-arg ref="scheduledExecutorService" />
		<constructor-arg ref="executorManager" />
		<constructor-arg ref="conversionService" />
		<constructor-arg ref="serverEndpoint" />
		<constructor-arg ref="terminalProxy" />
		<constructor-arg ref="addressableRegistry" />
		<constructor-arg ref="propertySynchronizer" />
		<constructor-arg ref="counterSynchronizer" />
	</bean>
	
	<!--<bean id="runnerServer" class="com.eviware.loadui.impl.agent.RunnerServer">
		<constructor-arg ref="serverEndpoint" />
	</bean>
	
	--><bean class="com.eviware.loadui.impl.discovery.DiscoveryResponder" destroy-method="release" />
</beans>